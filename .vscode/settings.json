{
    "files.exclude": {
        "**/out-arch/**": true,
        "out/**": true
    },
    "files.watcherExclude": {
        "**/.vuepress/dist/**": true,
        "out-arch/**": true,
        "out/**": true
    },
    "editor.smoothScrolling": true,
    "[vue]": {
        "editor.formatOnSave": false
    },
    "[json]": {
        "editor.formatOnSave": false
    },
    "go.lintFlags": [
        "internal",
        "cmd"
    ]
}
